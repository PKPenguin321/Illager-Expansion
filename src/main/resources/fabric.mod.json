{
  "schemaVersion": 1,
  "id": "sandbox",
  "version": "${version}",

  "name": "Sandbox Features",
  "description": "A 1.18.2 Fabric Mod that focuses on expanding content surrounding the mysterious Illagers in Minecraft!",
  "authors": [
    "Anyone who contributes :)"
  ],
  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/sandbox/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.sandbox.Sandbox"
    ],
    "client": [
      "me.sandbox.client.ClientRegistry"
    ],
    "mm:early_risers": [
      "me.sandbox.EarlyRiser"
    ]
  },
  "mixins": [
    "sandbox.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.13.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17",
    "mm": ">=2.3"
  },
  "suggests": {
    "another-mod": "*"
  },
  "accessWidener" : "sandbox.accesswidener"
}