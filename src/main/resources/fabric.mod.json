  "authors": [
    "Dricky",
    "All other contributors :)"
  ],
  "contact": {
    "sources": "https://github.com/OhDricky/Illager-Expansion",
    "issues": "https://github.com/OhDricky/Illager-Expansion/issues",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/illager-expansion-fabric"
  },

  "license": "MIT",
  "icon": "assets/illagerexp/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.sandbox.IllagerExpansion"
    ],
    "client": [
      "me.sandbox.client.ClientRegistry"
    ],
